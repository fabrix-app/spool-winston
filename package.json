  "devDependencies": {
    "@fabrix/fabrix": "^1.1.4",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@types/lodash": "^4.14.109",
    "@types/node": "~10.3.4",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "tslib": "~1.9.0",
    "tslint": "~5.10.0",
